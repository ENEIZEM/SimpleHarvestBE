{
    "java.dependency.packagePresentation": "flat",
    "java.dependency.syncWithFolderExplorer": true,
    "files.exclude": {
        "**/__pycache__/**": true,
        "**/tsconfig.json": true,
        "toolchain/bin": true,
        "toolchain/adb": true,
        "toolchain/build/gcc": true,
        "toolchain/build/gradle": true,
        "toolchain/build/lock": true,
        "**/*.log": true,
        ".settings": true
    },
    "java.project.referencedLibraries": [
        "toolchain/classpath/*.jar",
        "src/java/**/lib/*.jar"
    ]
}